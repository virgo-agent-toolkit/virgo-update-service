#!/usr/bin/env node

var fs = require('fs');
var _ = require('underscore');
var entry = require('../lib/entry').entry;
var log = require('logmagic').local('virgo-upgrade-service');

var default_options = {
  pkgcloud: { provider: 'rackspace' },
  secret: '',
  data_dir: '/tmp/data',
  exe_dir: '/tmp/deploy',
  service_name: 'virgo-upgrade-service',
  addr_host: null,
  addr_port: null,
  bind_host: '127.0.0.1',
  bind_port: 34000,
  etcd_host: '127.0.0.1',
  version_cache_timeout: 5 * 60,
  etcd_port: 4001,
  default_channels: [],
  default_channel_version: ''
};


function main() {
  entry(_.defaults({}, default_options));
}

main();
